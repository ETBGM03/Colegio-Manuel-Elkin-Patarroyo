<div class="row">
  <div class="col-md-6">
    <%= simple_form_for @usuario do |f| %>
    <%= f.input :nombre %>
    <%= f.input :apellido %>
    <%= f.input :telefono %>
    <%= f.input :direccion %>
    <%= f.input :email %>
    <%= f.input :password %>
     <%= f.input :password_confirmation %>
    <%= f.button :submit, class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
